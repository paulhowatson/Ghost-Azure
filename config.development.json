{
	"url": "https://paulhowatson.azurewebsites.net",
	"server": {
		"host": "127.0.0.1"
    },
    "database": {
        "client": "sqlite3",
        "connection": {
            "filename": "content/data/ghost.db"
        },
        "debug": false
    },
    "paths": {
        "contentPath": "content/"
    },
    "privacy": {
        "useRpcPing": false,
        "useUpdateCheck": true
    },
    "useMinFiles": false,
    "caching": {
        "theme": {
            "maxAge": 1000
        },
        "admin": {
            "maxAge": 1000
        }
	},
	"forceAdminSSL": true
}